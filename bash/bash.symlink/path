if [ -d /usr/local/bin ] ; then
	PATH=/usr/local/bin:"${PATH}"
fi

if [ -d ~/bin ] ; then
	PATH=~/bin:"${PATH}"
fi

if [ -d /Applications/SenchaSDKTools-2.0.0-Beta ] ; then
	PATH=/Applications/SenchaSDKTools-2.0.0-Beta:/Applications/SenchaSDKTools-2.0.0-Beta/jsbuilder:/Applications/SenchaSDKTools-2.0.0-Beta/bin:"${PATH}"
fi
